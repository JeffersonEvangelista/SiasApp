{
  "expo": {
    "name": "SiasApp",
    "slug": "siasapp",
    "version": "2.2.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F07A26"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.siasmobile"
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F07A26"
      },
      "package": "com.example.siasmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5ece6ff0-7063-4a9a-a02c-8397368de963"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "owner": "wesley2.0"
  }
}
